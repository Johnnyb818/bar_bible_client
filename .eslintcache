left\"\n                open={open}\n                onClose={() => setOpen(false)}\n            >\n                <div style={{ height : \"100%\", width : \"260px\", backgroundColor : \"#E79C03\" }}>\n                <Grid className={classes.navGrid}>\n                    <Link className={classes.paper} to=\"/\"><a>Home</a></Link>\n                </Grid>\n                <Grid className={classes.navGrid}>\n                    <Link className={classes.paper} to=\"/drinks\"><a>Cocktail Recipes</a></Link>\n                </Grid>\n                <Grid className={classes.navGrid}>\n                    <Link className={classes.paper} to=\"/syrups\"><a>Syrup Recipes</a></Link>\n                </Grid>\n                <Grid className={classes.navGrid}>\n                    <Link className={classes.paper} to=\"/tools\"><a>Bar Gear</a></Link>\n                </Grid>\n                <Grid className={classes.navGrid}>\n                    <Link className={classes.paper} to=\"/videos\"><a>Tutorials</a></Link>\n                </Grid>\n                </div>\n            </Drawer>\n        </div>\n    )\n}\n\n\nexport default NavBar;",{"ruleId":"53","replacedBy":"54"},{"ruleId":"55","replacedBy":"56"},{"ruleId":"57","severity":1,"message":"58","line":3,"column":17,"nodeType":"59","messageId":"60","endLine":3,"endColumn":21},{"ruleId":"61","severity":1,"message":"62","line":43,"column":125,"nodeType":"63","endLine":43,"endColumn":140},{"ruleId":"61","severity":1,"message":"62","line":24,"column":126,"nodeType":"63","endLine":24,"endColumn":141},{"ruleId":"64","severity":1,"message":"65","line":7,"column":13,"nodeType":"66","endLine":7,"endColumn":190},{"ruleId":"67","severity":1,"message":"68","line":58,"column":60,"nodeType":"66","endLine":58,"endColumn":63},{"ruleId":"67","severity":1,"message":"68","line":61,"column":66,"nodeType":"66","endLine":61,"endColumn":69},{"ruleId":"67","severity":1,"message":"68","line":64,"column":66,"nodeType":"66","endLine":64,"endColumn":6