syrups/\")\n      .then((response) => response.json())\n      .then((data) => {\n        console.log(data)\n        setSyrup(data);\n      })\n  }\n\n  const getVideo = () => {\n    fetch(url + \"/videos/\")\n      .then((response) => response.json())\n      .then((data) => {\n        console.log(data)\n        setVideo(data);\n      })\n  }\n  const getTool = () => {\n    fetch(url + \"/tools/\")\n      .then((response) => response.json())\n      .then((data) => {\n        console.log(data)\n        setTool(data);\n      })\n  }\n\n\n   // UseEffect to do initial fetch of gear\n   React.useEffect(() => getDrink(), []);\n   React.useEffect(() => getSyrup(), []);\n   React.useEffect(() => getVideo(), []);\n   React.useEffect(() => getTool(), []);\n   \n\n  return (\n    <div>\n      <NavBar/>    \n          \n      <main>\n        <Switch>\n          <Route exact path=\"/\" render={(rp) => <Home/>} />\n          <Route exact path=\"/drinks\" render={(rp) => <Drink {...rp} drink={drink}/>} />\n          <Route exact path=\"/syrups\" render={(rp) => <Syrup {...rp} syrup= {syrup}/>}/>          \n          <Route exact path=\"/tools\" render={(rp) => <Tool {...rp} tool= {tool}/>}/>          \n          <Route exact path=\"/videos\" render={(rp) => <Video {...rp} video={video}/>}/>\n        </Switch>\n      </main>      \n    </div>\n  );\n}\n\nexport default App;\n","/Users/johnnyb/Desktop/Development/capstone_project/BarBible/bar-bible-client/src/reportWebVitals.js",[],"/Users/johnnyb/Desktop/Development/capstone_project/BarBible/bar-bible-client/src/pages/Drink.js",[],"/Users/johnnyb/Desktop/Development/capstone_project/BarBible/bar-bible-client/src/pages/Syrup.js",[],"/Users/johnnyb/Desktop/Development/capstone_project/BarBible/bar-bible-client/src/pages/Tool.js",["47"],"import React from \"react\";\nimport { makeStyles } from '@material-ui/core/styles';\nimport Card from '@material-ui/core/Card';\nimport CardContent from '@material-ui/core/CardContent';\nimport CardMedia from '@material-ui/core/CardMedia';\nimport Typography from '@material-ui/core/Typography';\nimport Container from '@material-ui/core/Container';\nimport Button from '@material-ui/core/Button';\n\n\nconst Tool = (props) => {\n\n    const useStyles = makeStyles({\n        root: {\n          maxWidth: 375,\n          margin: \"4%\",        \n        },\n        media: {\n          height: 250,\n        },\n      });\n\n    const classes = useStyles();\n\n    const {tool} = props\n\n    const loadTool = () => (\n        \n        <Container maxWidth=\"sm\">\n            {tool.map((tool) => (\n                <Card className={classes.root}>\n                    <CardMedia\n                        className={classes.media}\n                        image={tool.img}\n                    />\n                    <CardContent>\n                        <Typography gutterBottom variant=\"h5\" component=\"h3\" style={{ fontWeight : \"bolder\", fontSize : \"25px\" }}>\n                            {tool.name}\n                        </Typography>\n                        <hr/>\n                        <p>{tool.description}</p>\n                        <h3>{tool.price}</h3>\n                        <Button variant=\"contained\"><a href={tool.url} style={{ color : \"black\", textDecoration : \"none\" }} target=\"_blank\" className=\"button\">Buy</a></Button>\n                    </CardContent>               \n                </Card>\n            ))}\n        </Container>\n    )\n    const loading = <h1>Loading...</h1>\n    return tool.length > 0 ? loadTool() : loading  \n}\n\nexport default Tool","/Users/johnnyb/Desktop/Development/capstone_project/BarBible/bar-bible-client/src/pages/Video.js",["48"],"import React from \"react\";\nimport Card from '@material-ui/core/Card';\nimport CardContent from '@material-ui/core/CardContent';\nimport Typography from '@material-ui/core/Typography';\nimport Container from '@material-ui/core/Container';\nimport Button from '@material-ui/core/Button';\n\n\nconst Video = (props) => {\n\n    const {video} = props\n\n    const loadVideo = () => (\n        \n        <Container maxWidth=\"sm\" style={{ padding : \"3%\" }}>\n            {video.map((video) => (\n                <Card style={{ margin : \"30px\"}}>\n                    <CardContent>\n                        <Typography style={{ fontWeight : \"bolder\", fontSize : \"25px\" }}>\n                            {video.name}\n                        </Typography>\n                        <hr/>\n                        <p>What will I learn?</p><p>{video.description}</p>\n                        <Button variant=\"contained\"><a href={video.url} style={{ color : \"black\", textDecoration : \"none\" }} target=\"_blank\">Check it out</a></Button>\n                    </CardContent>\n                </Card>\n            ))}\n        </Container>\n    )\n    const loading = <h1>Loading...</h1>\n    return video.length > 0 ? loadVideo() : loading  \n}\n\nexport default Video","/Users/johnnyb/Desktop/Development/capstone_project/BarBible/bar-bible-client/src/pages/Home.js",["49"],"import React from \"react\";\nimport Container from '@material-ui/core/Container';\n\nconst Home = (props) => {\n    return (\n        <Container className=\"home-logo\" maxWidth=\"md\">\n            <img src=\"https://images.unsplash.com/photo-1587223962281-365b9c129fcb?ixid=MXwxMjA3fDB8MHxzZWFyY2h8Mjd8fGNvY2t0YWlsfGVufDB8fDB8&ixlib=rb-1.2.1&auto=format&fit=crop&w=500&q=60\"></img>               \n        </Container>\n    )\n}\n\nexport default Home","/Users/johnnyb/Desktop/Development/capstone_project/BarBible/bar-bible-client/src/components/NavBar.js",["50","51","52","53","54"],"import React, { useState }  from \"react\";\nimport {Link} from \"react-router-dom\"\nimport { AppBar, Toolbar, IconButton, Typography, Drawer } from \"@material-ui/core\"\nimport { Menu } from \"@material-ui/icons\"\nimport Grid from '@material-ui/core/Grid';\nimport { makeStyles } from '@material-ui/core/styles';\n\n\nconst useStyles = makeStyles((theme) => ({\n    \n    paper: {\n      padding: '0 9%',\n      textAlign: 'center',\n      color: 'black',\n      textDecoration: 'none',\n      fontSize: '27px',       \n    },\n\n    navGrid: {\n      marginRight: 'auto',\n      marginLeft: 'auto',\n      alignItems: 'center',\n      marginTop: '10px', \n      \n    }\n  }));\n\nfunction NavBar() {\n    const [open, setOpen] = useState(false)\n    const classes = useStyles();\n\n    const handleDrawer = () => {\n      setOpen(true)\n    }\n      \n \n\n    return (\n        <div>\n            <AppBar style={{ backgroundColor : '#E79C03'}} position=\"static\">\n                <Toolbar>\n                    <IconButton style={{color : \"black\"}} onClick={handleDrawer} edge=\"start\" color=\"inherit\" aria-label=\"menu\">\n                        <Menu />\n                    </IconButton>\n                    <Typography style={{color : \"black\"}} variant=\"h6\">\n                        Bar Bible\n                    </Typography>\n                </Toolbar>\n            </AppBar>\n\n            <Drawer\n                anchor=\"left\"\n                open={open}\n                onClose={() => setOpen(false)}\n            >\n                <div style={{ height : \"100%\", width : \"260px\", backgroundColor : \"#E79C03\" }}>\n                <Grid className={classes.navGrid}>\n                    <Link className={classes.paper} to=\"/\"><a>Home</a></Link>\n                </Grid>\n                <Grid className={classes.navGrid}>\n                    <Link className={classes.paper} to=\"/drinks\"><a>Cocktail Recipes</a></Link>\n                </Grid>\n                <Grid className={classes.navGrid}>\n                    <Link className={classes.paper} to=\"/syrups\"><a>Syrup Recipes</a></Link>\n                </Grid>\n                <Grid className={classes.navGrid}>\n                    <Link className={classes.paper} to=\"/tools\"><a>Bar Gear</a></Link>\n    